{
  "name": "xd2svg",
  "version": "0.7.2",
  "description": "Utility for converting Adobe XD files (*.xd) to SVG",
  "keywords": [
    "svg",
    "xd",
    "adobe xd",
    "adobe experience design",
    "converter",
    "vector",
    "graphics"
  ],
  "homepage": "https://github.com/L2jLiga/xd2svg#xd2svg",
  "bugs": {
    "url": "https://github.com/L2jLiga/xd2svg/issues"
  },
  "license": "MIT",
  "author": "Andrey Chalkin <L2jLiga@gmail.com> (https://t.me/L2jLiga)",
  "directories": {
    "bin": "./bin",
    "example": "./example",
    "lib": "./lib",
    "man": "./man",
    "test": "./test"
  },
  "files": [
    "bin",
    "lib",
    "man"
  ],
  "main": "lib/xd2svg.js",
  "man": [
    "./man/xd2svg.1",
    "./man/cli.1"
  ],
  "bin": {
    "xd2svg-cli": "bin/xd2svg-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/L2jLiga/xd2svg.git"
  },
  "scripts": {
    "build": "yarn run build:core && yarn run build:browser",
    "build:browser": "webpack",
    "build:core": "tsc -p tsconfig.core.json",
    "clean": "rimraf lib",
    "lint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts'",
    "test": "cross-env TS_NODE_PROJECT=test/tsconfig.json mocha --require ts-node/register 'test/**/*.spec.ts'",
    "test:coverage": "nyc yarn test",
    "test:watch": "yarn test --watch-extensions ts --watch",
    "prepublish": "yarn run lint && yarn test && yarn run clean && yarn run build"
  },
  "dependencies": {
    "extract-zip": "1.6.7",
    "jsdom": "11.11.0",
    "svgo": "1.0.5",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/extract-zip": "1.6.2",
    "@types/jsdom": "11.0.6",
    "@types/mocha": "5.2.4",
    "@types/node": "8.10.20",
    "@types/sinon": "5.0.1",
    "@types/svgo": "1.0.1",
    "@types/tmp": "0.0.33",
    "chai": "4.1.2",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.2.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "rimraf": "2.6.2",
    "sinon": "6.1.3",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "typescript": "2.9.2",
    "webpack": "4.16.0",
    "webpack-cli": "3.0.8"
  },
  "engines": {
    "node": ">= 8.6.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/models/**"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov"
    ]
  },
  "preferGlobal": true
}
